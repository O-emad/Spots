@model AdminPanel.Models.VendorGalleryModel

<div class="col-sm-3 col-lg-3 col-md-3">
    <div class="thumbnail img-thumbnail">
        <img src="@("https://localhost:44308/api/images/"+Model.FileName)" alt="" class="rounded">
        <div class="caption">
            <h3>
                <a asp-controller="Vendor" asp-action="DeleteGallery" 
